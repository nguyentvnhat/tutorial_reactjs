    "parser": "babel-eslint",
    "extends": [
      "prettier/react",
      "prettier"
    ],
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "react/no-deprecated": 0,
        "space-before-blocks": 1
    },
    "parserOptions": {
        "sourceType": "module"
    }
}
